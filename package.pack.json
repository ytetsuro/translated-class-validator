{
  "name": "translated-class-validator",
  "version": "0.0.3",
  "description": "This repository translates class-validator as thinly-wrapped as possible.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ytetsuro/translated-class-validator.git"
  },
  "type": "module",
  "exports": {
    "./*": {
      "types": "./*.d.ts",
      "node": {
        "require": "./*.cjs.js",
        "import": "./*.mjs"
      },
      "default": "./*.esm.js"
    }
  },
  "keywords": ["class-validator", "translate"],
  "author": "iBotchME (@ytetsuro)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ytetsuro/translated-class-validator/issues"
  },
  "homepage": "https://github.com/ytetsuro/translated-class-validator#readme",
  "peerDependencies": {
    "class-validator": "*"
  },
  "devDependencies": {}
}
